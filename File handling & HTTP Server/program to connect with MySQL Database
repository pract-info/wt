var mysql = require('mysql');

var conn = mysql.createConnection({
    host: 'localhost',
    user: 'root',
    password: 'pragati',
    database: 'MCA'
});

conn.connect(function(error) {
    if (error) throw error;
    console.log("Connected!");
});

